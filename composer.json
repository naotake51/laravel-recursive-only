{
    "name": "naotake51/laravel-recursive-only",
    "description": "Register recursiveOnly() extending only() to Model, Collection and Arr.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Naotake51\\RecursiveOnly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naotake51\\RecursiveOnly\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "n-takeshita",
            "email": "naokisyuu@gmail.com"
        }
    ],
    "keywords": ["laravel", "library", "only", "macro", "model", "eloquent", "illuminate", "collection"],
    "require": {
        "php": "^7.1 || ^8.0",
        "illuminate/database": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naotake51\\RecursiveOnly\\RecursiveOnlyServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
